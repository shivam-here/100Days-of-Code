int pivotIndex(vector<int>& nums) {
        
        int lsum=0;
        int rsum=accumulate(nums.begin(),nums.end(),0);
        for(int i=0; i<nums.size();i++)
        {
             rsum-=nums[i];
            
               if(lsum==rsum)
                   return i;
            
               else
               {
                   lsum+=nums[i];
               }
        }
        return -1;
        
    }
